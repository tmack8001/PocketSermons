<resource>
    <!-- End current session if app sleeps for a period of time -->
    <integer name="ga_sessionTimeout">300</integer>

    <!-- Enable automatic Activity measurement -->
    <bool name="ga_autoActivityTracking">true</bool>

    <!--  The property id associated with this analytics tracker -->
    <string name="ga_trackingId">UA-55404743-1</string>

    <!-- Report uncaught exceptions -->
    <bool name="ga_reportUncaughtExceptions">true</bool>

    <!-- The screen names that will appear in reports -->
    <screenName name="com.tmack.pocketsermons.browser.SermonBrowserActivity">
        TheMount SermonBrowserView
    </screenName>
    <screenName name="com.tmack.pocketsermons.mediaPlayer.LocalVideoActivity">
        TheMount SermonVideoView
    </screenName>
</resource>